#: ../gio/gdbusauthmechanismsha1.c:681
#, c-format
msgid "Error opening keyring '%s' for writing: "
msgstr "Errorea '%s' gako sorta idazteko irekitzean: "

#: ../gio/gdbusauthmechanismsha1.c:878
