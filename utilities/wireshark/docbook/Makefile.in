	$(XSLTPROC) --stringparam paper.type letter \
		$(COMMON_XSLTPROC_ARGS) $(WSUG_PDF_XSLTPROC_ARGS) \
		--nonet $(srcdir)/custom_layer_pdf.xsl $< > $@

